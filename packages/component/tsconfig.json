{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "target": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "jsx": "react",
    "paths": {
      "@/*": [
        "src/*"
      ],
    }
  },
  "include": [
    "src/**/*"
  ]
}